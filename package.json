{
  "name": "@ban-team/ban-plateforme",
  "version": "1.0.0",
  "description": "Plateforme d'intégration et de production de la Base Adresse Nationale",
  "repository": "https://github.com/etalab/ban-plateforme",
  "author": "Jérôme Desboeufs <jerome.desboeufs@data.gouv.fr>",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build":"babel src -d build --copy-files",
    "build-watch":"babel --watch src -d build --copy-files",
    "apply-batch-certification": "node build/scripts/apply-batch-certification",
    "prepare-contours": "node build/scripts/prepare-contours",
    "download-datasets": "node build/scripts/download-datasets",
    "import:ign-api-gestion": "node build/lib/import/cli ign-api-gestion",
    "import:cadastre": "node build/lib/import/cli cadastre",
    "import:ftth": "node build/lib/import/cli ftth",
    "dist": "node build/lib/distribute/cli",
    "compose": "node build/lib/compose/cli",
    "lint": "xo",
    "test": "jest",
    "worker": "node worker",
    "worker:dev": "yarn build-watch && npx nodemon worker",
    "start": "node server",
    "dev": "yarn build-watch && npx nodemon server"
  },
  "dependencies": {
    "@ban-team/validateur-bal": "^2.13.0",
    "@etalab/adresses-util": "^0.8.2",
    "@etalab/decoupage-administratif": "^3.0.0",
    "@ban-team/fantoir": "^0.15.0",
    "@ban-team/gazetteer": "^2.0.0",
    "@etalab/majic": "^0.11.0",
    "@etalab/normadresse": "^1.2.0",
    "@etalab/project-legal": "^0.6.0",
    "@keyv/sqlite": "^3.5.2",
    "@mapbox/tilebelt": "^1.0.2",
    "@turf/turf": "^6.5.0",
    "bluebird": "^3.7.2",
    "bull": "^3.29.3",
    "codes-postaux": "^4.0.0",
    "cors": "^2.8.5",
    "csv-write-stream": "^2.0.0",
    "date-fns": "^2.28.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.1",
    "epsg-index": "^1.2.0",
    "express": "^4.18.1",
    "fast-json-stable-stringify": "^2.1.0",
    "form-data": "^4.0.0",
    "fs-extra": "^10.1.0",
    "geojson-stream": "^0.1.0",
    "geojson-vt": "^3.2.1",
    "get-stream": "^6.0.1",
    "got": "^11.8.5",
    "gunzip-stream": "^1.0.1",
    "iconv-lite": "^0.6.3",
    "into-stream": "^6.0.0",
    "ioredis": "^5.0.6",
    "keyv": "^4.3.2",
    "leven": "^3.1.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "mongodb": "^4.7.0",
    "morgan": "^1.10.0",
    "ms": "^2.1.3",
    "nanoid": "^3.3.4",
    "ndjson": "^2.0.0",
    "ora": "^5.4.1",
    "papaparse": "^5.3.2",
    "proj4": "^2.8.0",
    "pumpify": "^2.0.1",
    "rev-hash": "^3.0.0",
    "vt-pbf": "^3.1.3",
    "worker-farm": "^1.7.0",
    "yup": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "jest": "^29.5.0",
    "xo": "^0.50.0"
  },
  "xo": {
    "envs": [
      "es2021",
      "node",
      "jest"
    ],
    "semicolon": false,
    "space": 2,
    "rules": {
      "comma-dangle": "off",
      "unicorn/no-array-reduce": "off",
      "unicorn/no-array-for-each": "off",
      "unicorn/import-style": "off",
      "unicorn/prevent-abbreviations": "off",
      "unicorn/prefer-module": "off",
      "unicorn/prefer-code-point": "off",
      "unicorn/no-await-expression-member": "off",
      "import/extensions": "off",
      "n/prefer-global/process": "off",
      "n/prefer-global/buffer": "off"
    }
  },
  "engines": {
    "node": ">= 14"
  }
}
